{
  "name": "testecommerce",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start --clear",
    "start:safe": "rm -rf node_modules/.cache && expo start --clear",
    "start:reset": "watchman watch-del-all 2>/dev/null || true && npm run start:safe",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "jest",
    "clean": "rm -rf node_modules .expo dist android/build ios/build",
    "clean:full": "npm run clean && rm -rf package-lock.json && npm install",
    "prebuild": "expo prebuild",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios"
  },
  "dependencies": {
    "@expo/vector-icons": "14.0.4",
    "@gorhom/bottom-sheet": "4.6.4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "6.6.1",
    "@react-navigation/drawer": "6.7.2",
    "@react-navigation/native": "6.1.18",
    "@react-navigation/native-stack": "6.10.1",
    "@shopify/flash-list": "1.7.3",
    "expo": "52.0.47",
    "expo-font": "13.0.4",
    "expo-image": "2.0.7",
    "expo-linear-gradient": "14.0.2",
    "expo-splash-screen": "0.29.24",
    "expo-status-bar": "2.0.1",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "2.20.2",
    "react-native-mmkv": "3.1.0",
    "react-native-reanimated": "3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "4.4.0",
    "react-native-svg": "15.8.0",
    "zustand": "4.5.5",
    "axios": "1.7.7",
    "expo-local-authentication": "15.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@types/react": "18.3.12",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "babel-plugin-module-resolver": "5.0.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.0.0",
    "eslint-plugin-react-native": "4.1.0",
    "prettier": "3.3.3",
    "typescript": "5.6.3"
  },
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}